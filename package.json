{
    "name": "cc",
    "displayName": "CC老师鼓励师",
    "description": "cp了杨超越鼓励师的代码，做一下test注明出处（自用）",
    "version": "0.0.3",
    "publisher": "原作者formulahendry，更改者cc",
    "icon": "logo.jpg",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "CC老师",
        "提醒",
        "鼓励"
    ],
    "bugs": {
        "url": "https://github.com/ChenChenJoke/vscode-ycy/issues",
        "email": "978563552@qq.com"
    },
    "homepage": "https://github.com/ChenChenJoke/vscode-ycy/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/ChenChenJoke/vscode-ycy.git"
    },
    "activationEvents": [
        "onCommand:ycy.showReminderView",
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "ycy.showReminderView",
                "title": "打开提醒页面",
                "category": "ycy"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "CC",
            "properties": {
                "ycy.reminderViewIntervalInMinutes": {
                    "type": "number",
                    "default": 30,
                    "description": "展示提醒页面的时间间隔（分钟）"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
